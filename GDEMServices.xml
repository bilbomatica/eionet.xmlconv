<?xml version="1.0" encoding="UTF-8"?>
<Services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="./services.xsd">
	<Service Name="ConversionService" Provider="eionet.gdem.ConversionService">
	<Methods>
			<Method Name="convert" Auth="false">
				<Parameters>
					<Parameter Name="url" Type="STRING"/>
					<Parameter Name="convertId" Type="STRING"/>
				</Parameters>
				<Value Type="STRUCT"/>
			</Method>
			<Method Name="listConversions" Auth="false">
				<Parameters>
					<Parameter Name="id" Type="STRING"/>
				</Parameters>
				<Value Type="ARRAY"/>
			</Method>
		</Methods>		
	</Service>
	<Service Name="XQueryService" Provider="eionet.gdem.XQueryService">
	<Methods>
			<Method Name="analyze" Auth="false">
				<Parameters>
					<Parameter Name="url" Type="STRING"/>
					<Parameter Name="xqScript" Type="STRING"/>
				</Parameters>
				<Value Type="STRING"/>
			</Method>
			<Method Name="getResult" Auth="false">
				<Parameters>
					<Parameter Name="jobId" Type="STRING"/>
				</Parameters>
				<Value Type="STRUCT"/>
					<!--Member Name="CODE" Type="INT"/>
					<Member Name="RESULT" Type="STRING"/>
				</Value-->
			</Method>
		</Methods>		
	</Service>
	<Service Name="ValidationService" Provider="eionet.gdem.ValidationService">
	<Methods>
			<Method Name="validate" Auth="false">
				<Parameters>
					<Parameter Name="url" Type="STRING"/>
				</Parameters>
				<Value Type="STRING"/>
			</Method>
			<Method Name="validateSchema" Auth="false">
				<Parameters>
					<Parameter Name="url" Type="STRING"/>
					<Parameter Name="schemaUrl" Type="STRING"/>
				</Parameters>
				<Value Type="STRING"/>
			</Method>
		</Methods>		
	</Service>
</Services>